<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-configuration PUBLIC "-//Hibernate/Hibernate Configuration DTD 3.0//EN" "http://www.hibernate.org/dtd/hibernate-configuration-3.0.dtd">
<hibernate-configuration>
 <session-factory>
    <property name="hibernate.transaction.factory_class">org.hibernate.transaction.JDBCTransactionFactory</property>
    <property name="hibernate.current_session_context_class">thread</property>
    <property name="hibernate.ogm.datastore.provider">org.hibernate.ogm.datastore.mongodb.impl.MongoDBDatastoreProvider</property>  
    <property name="hibernate.ogm.datastore.database">tangerine</property>   
       <property name="show_sql">false</property> 
    <property name="hibernate.ogm.datastore.port">27017</property>      
 
 <!--   <property name="hibernate.ogm.datastore.host">52.68.18.35</property> -->

    <property name="hibernate.ogm.datastore.host">192.168.10.186</property>   
    
	
        
  </session-factory>
</hibernate-configuration>